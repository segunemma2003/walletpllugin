{
  "name": "paycio-wallet",
  "version": "2.0.0",
  "description": "Secure multi-chain cryptocurrency wallet supporting Bitcoin, Ethereum, Solana, TRON, and more",
  "main": "dist/popup.js",
  "scripts": {
    "build": "node scripts/build.js",
    "build:chrome": "node scripts/build.js chrome",
    "build:firefox": "node scripts/build.js firefox",
    "build:edge": "node scripts/build.js edge",
    "build:all": "node scripts/build.js",
    "build:dev": "webpack --mode development --watch",
    "build:prod": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "dev:chrome": "webpack --mode development --watch --env browser=chrome",
    "dev:firefox": "webpack --mode development --watch --env browser=firefox",
    "dev:edge": "webpack --mode development --watch --env browser=edge",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check-all": "npm run lint && npm run type-check && npm test",
    "clean": "rm -rf dist",
    "package": "npm run clean && npm run build:all && npm run zip",
    "zip": "cd dist && zip -r paycio-wallet-chrome.zip chrome/ && zip -r paycio-wallet-firefox.zip firefox/ && zip -r paycio-wallet-edge.zip edge/",
    "analyze": "webpack --mode production --analyze",
    "optimize-images": "imagemin src/assets/* --out-dir=dist/assets",
    "minify": "terser dist/*.js -o dist/*.min.js",
    "security-audit": "npm audit && snyk test",
    "performance-test": "lighthouse dist/popup.html --output=json --output-path=./lighthouse-report.json",
    "deploy:chrome": "npm run build:chrome && npm run zip:chrome",
    "deploy:firefox": "npm run build:firefox && npm run zip:firefox",
    "deploy:edge": "npm run build:edge && npm run zip:edge",
    "zip:chrome": "cd dist && zip -r paycio-wallet-chrome.zip chrome/",
    "zip:firefox": "cd dist && zip -r paycio-wallet-firefox.zip firefox/",
    "zip:edge": "cd dist && zip -r paycio-wallet-edge.zip edge/",
    "prepublishOnly": "npm run check-all && npm run security-audit",
    "postinstall": "husky install"
  },
  "keywords": [
    "wallet",
    "cryptocurrency",
    "blockchain",
    "ethereum",
    "bitcoin",
    "solana",
    "tron",
    "browser-extension",
    "metamask",
    "multichain",
    "defi",
    "nft",
    "react",
    "typescript",
    "tailwindcss",
    "paycio"
  ],
  "author": "PayCio Wallet Team",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.0.18",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.34",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.95.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@walletconnect/sign-client": "^2.16.1",
    "@walletconnect/utils": "^2.16.1",
    "@walletconnect/types": "^2.16.1",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.6",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "ethers": "^6.13.2",
    "framer-motion": "^11.5.4",
    "hdkey": "^2.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.439.0",
    "moment": "^2.30.1",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "react-qr-code": "^2.0.15",
    "tailwind-merge": "^2.5.2",
    "ton": "^13.11.2",
    "ton-core": "^0.58.1",
    "ton-crypto": "^3.3.0",
    "uuid": "^10.0.0",
    "webextension-polyfill": "^0.12.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.5",
    "bcryptjs": "^2.4.3",
    "scrypt-js": "^3.0.1",
    "axios": "^1.7.7",
    "web3": "^4.12.1",
    "dotenv": "^16.4.5",
    "xrpl": "^3.0.0",
    "@ledgerhq/hw-transport-webusb": "^6.29.0",
    "@ledgerhq/hw-app-eth": "^6.37.3",
    "@trezor/connect": "^9.4.1",
    "@sentry/browser": "^8.30.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/chrome": "^0.0.270",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.4",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/webextension-polyfill": "^0.10.7",
    "@types/bcryptjs": "^2.4.6",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.4.45",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.11",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-bundle-analyzer": "^4.10.2",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "snyk": "^1.1293.1",
    "imagemin": "^9.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "terser": "^5.32.0",
    "cross-env": "^7.0.3",
    "lighthouse": "^12.2.1",
    "compression-webpack-plugin": "^11.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "imagemin-webpack-plugin": "^2.4.2"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run check-all"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/src/setupTests.ts"],
    "moduleNameMapping": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/setupTests.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/paycio-wallet.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/paycio-wallet/issues"
  },
  "homepage": "https://paycio-wallet.com",
  "funding": {
    "type": "patreon",
    "url": "https://patreon.com/payciowallet"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0"
  },
  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0"
  }
}